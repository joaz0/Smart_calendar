# Render.com Environment Variables
# Configure estas vari√°veis no dashboard do Render

# Database (Auto-configurado pelo Render)
DATABASE_URL=postgresql://user:xxx@host:5432/database

# JWT Secrets (Gerados automaticamente)
JWT_SECRET=your-jwt-secret-min-32-chars
JWT_REFRESH_SECRET=your-refresh-secret-min-32-chars

# Server
NODE_ENV=production
PORT=3000

# CORS
CORS_ORIGIN=https://smart-calendar.netlify.app

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# N8N Integration (Opcional)
N8N_URL=https://your-n8n-instance.com
N8N_API_KEY=your-n8n-api-key

# OAuth (Opcional)
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
